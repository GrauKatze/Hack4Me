<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>


<TilePane fx:id="root" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainController">>
    <Button fx:id="dataCraft" onAction="#CraftBuild" text="dataCraft" style="-fx-graphic: url(/Resource/dataCraft.png)" />
    <Button fx:id="dataSave" onAction="#dataSaveBuild" text="dataSave" style="-fx-graphic: url(/Resource/dataSave.png)" />
    <Button fx:id="Protector" onAction="#ProtectorBuild" text="Protector" style="-fx-graphic: url(/Resource/Protector.png)" />
</TilePane>